<%= content_tag_for :li, item, class: "result-item", item_id: item.id do -%>
  <div class="well well-small">
    <strong><%= item.title %></strong> <br />

    <ul class="unstyled">
    	<% item.variants_array.each_with_index do |variant, i| %>
	    	<li>
		      	<label class="radio">
					<input type="radio" disabled="disabled" <%= 'checked="true"' if result.to_i == i %>/>
					<%= variant %>
				</label>
			</li>
	    <% end %>
	</ul>
  </div>
<%- end -%>