<td class="reponse-item-title">
	<%= "#{counter}. #{item.title}" %>
</td>
<td class="reponse-content">
	<ul class="unstyled">
		<% item.variants_array.each_with_index do |variant, i| %>
	    	<li>
		      	<label class="radio">
					<input type="radio" disabled="disabled" <%= 'checked="true"' if result.to_i == i %>/>
					<%= variant %>
				</label>
			</li>
	    <% end %>
	</ul>
</td>