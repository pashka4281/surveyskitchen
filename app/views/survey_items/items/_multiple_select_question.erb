<%= content_tag_for :div, item, class: "insertable sortable_item question-item", item_id: item.id do -%>
	<div class="navbar-inner pull-right">
    	<%= render 'survey_items/items/_tools_box'%>
  	</div>
  	<div class="well well-small">
  		<strong><%= item.title%></strong>
  		<div class="clear"></div>
    	<ul class="unstyled">
	    	<% for var in item.variants %>
		    	<li>
			      	<label class="checkbox">
						<input type="checkbox" name="r_btn" value="<%= var %>" /><%= var %>
					</label>
				</li>
		    <% end %>
		</ul>
  	</div>
	
	<button class="btn btn-mini btn-block insert_buttons" type="button">Insert here Â»</button>
<% end %>
