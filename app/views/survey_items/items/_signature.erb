<%= content_tag_for :li, item, class: "survey_item signature-item", item_id: item.id do -%>
  <%= render 'survey_items/items/_tools_box', item: item %>
  <div class="item-inner">
    <strong class="item-title"><%= item.title %></strong>
    <div class="item-content">
      <div class="signature">
        <img src="/images/icons/signature.png">
      </div>
    </div>
  </div>
<%- end -%>