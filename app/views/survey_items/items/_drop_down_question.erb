<%= content_tag_for :div, item, class: "insertable sortable_item question-item", item_id: item.id do -%>
  <div class="navbar-inner pull-right">
      <%= render 'survey_items/items/_tools_box', item: item %>
  </div>
  <div class="well well-small">
    <strong><%= item.title %></strong>
    <div class="clear"></div>
    <select name="drop_down_<%= item.id %>" class="span9">
      <% for var in item.variants_array %>
        <option value="<%= var %>" > <%= var %> </option>
      <% end %>
    </select>
  </div>
  
  <%= insert_button %>
<%- end -%>
